{
  "name": "b08x.github.io",
  "version": "1.0.0",
  "description": "[![Deploy Jekyll site to Pages](https://github.com/b08x/b08x.github.io/actions/workflows/jekyll.yml/badge.svg)](https://github.com/b08x/b08x.github.io/actions/workflows/jekyll.yml)",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel watch:js dev:jekyll",
    "dev:jekyll": "bundle exec jekyll clean && bundle exec jekyll serve --incremental",
    "build": "npm-run-all build:js build:worker build:css build:jekyll",
    "build:js": "esbuild src/main.tsx --bundle --outfile=assets/js/dist/garden-widgets-v2.js --minify --sourcemap --target=es2020 --jsx=automatic --alias:react=./node_modules/react --alias:react-dom=./node_modules/react-dom",
    "build:worker": "esbuild src/components/graph.worker.ts --bundle --outfile=assets/js/dist/graph.worker.js --minify --sourcemap --target=es2020",
    "build:css": "npx tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/compiled.css --minify",
    "build:jekyll": "bundle exec jekyll clean && bundle exec jekyll build",
    "watch:js": "esbuild src/main.tsx --bundle --outfile=assets/js/dist/garden-widgets-v2.js --sourcemap --target=es2020 --watch --jsx=automatic --alias:react=./node_modules/react --alias:react-dom=./node_modules/react-dom",
    "watch:worker": "esbuild src/components/graph.worker.ts --bundle --outfile=assets/js/dist/graph.worker.js --sourcemap --target=es2020 --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/b08x/b08x.github.io.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/b08x/b08x.github.io/issues"
  },
  "homepage": "https://github.com/b08x/b08x.github.io#readme",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.19",
    "@types/d3": "^7.4.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.22",
    "esbuild": "^0.27.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "d3": "^7.9.0",
    "lucide-react": "^0.562.0",
    "mermaid": "^11.12.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "react-jsoncanvas": "github:Digital-Tvilling/react-jsoncanvas",
    "react-markdown": "^10.1.0",
    "react-photo-view": "^1.2.7",
    "react-player": "^2.16.0",
    "react-syntax-highlighter": "^15.6.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  }
}
